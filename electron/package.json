{
  "name": "duplex",
  "version": "1.0.0",
  "license": "MIT",
  "main": "build/main.js",
  "scripts": {
    "dev:build": "tsc",
    "prod:build": "tsc && electron-builder -mwl",
    "start": "yarn dev:build && electron ./build/main.js"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*",
      "assets/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "build/installer"
    }
  },
  "dependencies": {
  },
  "devDependencies": {
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5"
  }
}
